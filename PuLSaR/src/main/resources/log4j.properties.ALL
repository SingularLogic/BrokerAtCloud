log4j.rootLogger=INFO, stdout#, file#XXX: log4j.rootLogger=INFO, stdoutlog4j.logger.eu.brokeratcloud=ALLlog4j.logger.eu.brokeratcloud.persistence=INFOlog4j.logger.eu.brokeratcloud.opt.RecommendationManager=ALLlog4j.logger.eu.brokeratcloud.opt.rank.ahp=ALLlog4j.logger.eu.brokeratcloud.rest.UserFacingWS=ALLlog4j.logger.eu.brokeratcloud.rest.BrokerManamentWS=ALLlog4j.logger.eu.brokeratcloud.rest.opt.AuxiliaryService=ALL# Console appenderlog4j.appender.stdout=org.apache.log4j.ConsoleAppenderlog4j.appender.stdout.layout=org.apache.log4j.PatternLayout#XXX:log4j.appender.stdout.layout.ConversionPattern=[%p] %C{1}.%M: %m%nlog4j.appender.stdout.layout.ConversionPattern=[%p] %m%n#log4j.appender.stdout.layout.ConversionPattern=[%p] %m%n# Rolling file appenderlog4j.appender.file=org.apache.log4j.DailyRollingFileAppenderlog4j.appender.file.layout=org.apache.log4j.PatternLayout#log4j.appender.file.layout.ConversionPattern=%d %p %c [%C:%M][%F:%L] - %m%nlog4j.appender.file.layout.ConversionPattern=%d{HH:mm:ss.SSS} [%p] %c{1} %C{1}.%M: %m%n# ... Log files settingslog4j.appender.file.File=logs/pulsar-output.loglog4j.appender.file.DatePattern='.' yyyy-MM-dd#log4j.appender.file.MaxFileSize=10MB#log4j.appender.file.maxBackupIndex=0