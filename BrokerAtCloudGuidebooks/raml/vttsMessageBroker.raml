/topics/evaluation/vttsValidation/{clientId}:
  description: Request a VTTS Validation.
  uriParameters:
    clientId:
      description: An identifier of the client that is subscribing.
  post:
    description: Send a VTTS Model to trigger a Validation
    body:
      application/xml:
    responses:
      200:
        body:
          application/xml:
/topics/evaluation/vttsVerification/{clientId}:
  description: Request a VTTS Verification.
  uriParameters:
