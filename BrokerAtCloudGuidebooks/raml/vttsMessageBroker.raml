        responses:
          204:
  /durable/vttsVerificationReport/{clientId}/unsubscribe:
      description: Unsubscribe from Durable Subscription to VTTS Verification results.
      uriParameters:
        clientId:
          description: An identifier of the client that is subscribing.
      put:
        description: Durable Topic unsubscription
        responses:
          204:
  /evaluation/vttsGenerationReport/{clientId}:
