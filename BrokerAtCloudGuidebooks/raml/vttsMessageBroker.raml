#%RAML 0.8
title: Functional Testing API
baseUri: http://example.com
documentation:
  - title: Functional Testing API
    content: The interfaces of the Functional Testing mechanism are based on the pub/sub system.
/subscriptions:    
  description: Topic Subscription Management
  /evaluation/vttsValidationReport/{clientId}:
    description: Temporary Subscription to VTTS Validation results.
    uriParameters:
      clientId:
        description: An identifier of the client that is subscribing.
    put:
      description: Topic Subscription Interfaces
      queryParameters:
        wsCallbackEndpoint:
          description: A valid WS endpoint which will receive a POST request when a new message arrives for the subscribed topic. 
      responses:
        204:
  /durable/vttsValidationReport/{clientId}:
      description: Durable Subscription to VTTS Validation results.
      uriParameters:
        clientId:
          description: An identifier of the client that is subscribing.
      put:
        description: Durable Topic Subscription
        queryParameters:
          wsCallbackEndpoint:
            description: A valid WS endpoint which will receive a POST request when a new message arrives for the subscribed topic. 
        responses:
          204:
  /durable/vttsValidationReport/{clientId}/unsubscribe:
      description: Unsubscribe from Durable Subscription to VTTS Validation results.
      uriParameters:
        clientId:
          description: An identifier of the client that is subscribing.
      put:
        description: Durable Topic unsubscription
        responses:
          204:
